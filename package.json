{
  "name": "QualityJam-Todos",
  "version": "0.1.23",
  "description": "Kitsune UI",
  "engines": {
    "node": "5.6.0"
  },
  "scripts": {
    "clean": "rimraf node_modules typings/main* typings/browser* doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist && webpack --progress --profile",
    "server": "webpack-dev-server --inline --progress --port 8080",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tsconfig-lint",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run test",
    "docs": "typedoc --options typedoc.json src/app/app.ts",
    "start": "npm run server",
    "postinstall": "npm run webdriver-update && typings install && npm run temp-fix-angular-polyfills-1 && npm run temp-fix-angular-polyfills-2",
    "temp-fix-angular-polyfills-1": "replace -i \"//throw new Error\\('More tasks executed then were scheduled.'\\);\" \"throw new Error('More tasks executed then were scheduled.');\" \"./node_modules/angular2/bundles/angular2-polyfills.js\"",
    "temp-fix-angular-polyfills-2": "replace -i \"throw new Error\\('More tasks executed then were scheduled.'\\);\" \"//throw new Error('More tasks executed then were scheduled.');\" \"./node_modules/angular2/bundles/angular2-polyfills.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QAS-Labs/QualityJam-Todos.git"
  },
  "author": "QASymphony",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "bugs": {
    "url": "https://github.com/QAS-Labs/QualityJam-Todos/issues"
  },
  "homepage": "https://github.com/QAS-Labs/QualityJam-Todos#readme",
  "dependencies": {
    "angular2": "^2.0.0-beta.13",
    "dragula": "3.6.8",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "jquery": "^2.2.2",
    "karma": "^0.13.22",
    "lodash": "^4.8.2",
    "moment": "^2.12.0",
    "ng2-dragula": "^1.1.5",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "semantic-ui-css": "^2.1.8",
    "zone.js": "^0.6.9"
  },
  "devDependencies": {
    "angular-cli": "0.0.*",
    "autoprefixer": "^6.3.6",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "git-rev": "^0.2.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.4.0",
    "json-loader": "^0.5.4",
    "karma": "0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-junit-reporter": "^0.4.1",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "node-sass": "^3.4.2",
    "null-loader": "git+https://github.com/webpack/null-loader.git",
    "postcss-loader": "^0.8.2",
    "protractor": "^3.2.2",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.5.1",
    "replace": "^0.3.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.1",
    "tsconfig-lint": "^0.11.0",
    "tslint": "^3.7.0",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.9",
    "typings": "^0.7.12",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
